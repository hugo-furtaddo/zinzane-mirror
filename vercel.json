{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
  
    "builds": [
      { "src": "clone-all/**/*.html", "use": "@vercel/static" },
      { "src": "clone-all/**/*.css",  "use": "@vercel/static" },
      { "src": "clone-all/**/*.js",   "use": "@vercel/static" },
      { "src": "clone-all/**/*.{png,jpg,jpeg,gif,svg}", "use": "@vercel/static" },
      { "src": "clone-all/**/*.{woff,woff2,ttf,eot}",   "use": "@vercel/static" }
    ],

    "redirects": [
      {
        "source": "/",
        "destination": "/clone-all/",
        "statusCode": 302
      }
    ],
  

    "rewrites": [
      {
        "source": "/clone-all",
        "destination": "/clone-all/index.html"
      },
      {
        "source": "/clone-all/:path*/",
        "destination": "/clone-all/:path*/index.html"
      }
    ],
  

    "cleanUrls": true,
    "trailingSlash": true,

    "outputDirectory": "."
  }
  